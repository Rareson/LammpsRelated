@misc{youngs2014,
	title = {Young's modulus, Wikipedia},
	copyright = {Creative Commons Attribution-{ShareAlike} License},
	url = {http://en.wikipedia.org/w/index.php?title=Young%27s_modulus&oldid=625853216},
	abstract = {Young's modulus, also known as the tensile modulus or elastic modulus, is a measure of the stiffness of an elastic material and is a quantity used to characterize materials. It is defined as the ratio of the stress (force per unit area) along an axis over the strain (ratio of deformation over initial length) along that axis in the range of stress in which Hooke's law holds.[1]},
