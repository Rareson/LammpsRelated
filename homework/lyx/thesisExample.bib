@article{jiangYoungs2009,
	title = {Young's modulus of Graphene: a molecular dynamics study},
	volume = {80},
	issn = {1098-0121, 1550-235X},
	shorttitle = {Young's modulus of Graphene},
	url = {http://arxiv.org/abs/0906.5237},
	doi = {10.1103/PhysRevB.80.113405},
	abstract = {The Young's modulus of graphene is investigated through the intrinsic thermal vibration in graphene which is `observed' by molecular dynamics, and the results agree quite well with the recent experiment [Science {\textbackslash}textbf\{321\}, 385 (2008)]. This method is further applied to show that the Young's modulus of graphene: 1. increases with increasing size and saturation is reached after a threshold value of the size; 2. increases from 0.95 {TPa} to 1.1 {TPa} as temperature increases in the region [100, 500]K; 3. is insensitive to the isotopic disorder in the low disorder region (\${\textless} 5\%\$), and decreases gradually after further increasing the disorder percentage.},
	number = {11},
	urldate = {2014-10-04},
	journal = {Physical Review B},
	author = {Jiang, Jin-Wu and Wang, Jian-Sheng and Li, Baowen},
	month = sep,
	year = {2009},
	note = {{arXiv}: 0906.5237},
	keywords = {Condensed Matter - Materials Science},
	file = {arXiv\:0906.5237 PDF:/home/domicor/.mozilla/firefox/7tlpeovq.default/zotero/storage/TIAX8DG8/Jiang et al. - 2009 - Young's modulus of Graphene a molecular dynamics .pdf:application/pdf;arXiv.org Snapshot:/home/domicor/.mozilla/firefox/7tlpeovq.default/zotero/storage/636W5PRE/0906.html:text/html}
}

@misc{lattice2014,
	title = {Lattice constant, Wikipedia},
	copyright = {Creative Commons Attribution-{ShareAlike} License},
	url = {http://en.wikipedia.org/w/index.php?title=Lattice_constant&oldid=608331410},
	abstract = {The lattice constant, or lattice parameter, refers to the physical dimension of unit cells in a crystal lattice. Lattices in three dimensions generally have three lattice constants, referred to as a, b, and c. However, in the special case of cubic crystal structures, all of the constants are equal and we only refer to a. Similarly, in hexagonal crystal structures, the a and b constants are equal, and we only refer to the a and c constants. A group of lattice constants could be referred to as lattice parameters. However, the full set of lattice parameters consist of the three lattice constants and the three angles between them.},
	language = {en},
	urldate = {2014-10-04},
