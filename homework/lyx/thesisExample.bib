@article{tersoffNew1988,
	title = {New empirical approach for the structure and energy of covalent systems},
	volume = {37},
	url = {http://link.aps.org/doi/10.1103/PhysRevB.37.6991},
	doi = {10.1103/PhysRevB.37.6991},
	abstract = {Empirical interatomic potentials permit the calculation of structural properties and energetics of complex systems. A new approach for constructing such potentials, by explicitly incorporating the dependence of bond order on local environment, permits an improved description of covalent materials. In particular, a new potential for silicon is presented, along with results of extensive tests which suggest that this potential provides a rather realistic description of silicon. The limitations of the potential are discussed in detail.},
	number = {12},
	urldate = {2014-10-04},
	journal = {Physical Review B},
	author = {Tersoff, J.},
	month = apr,
	year = {1988},
	pages = {6991--7000},
	file = {APS Snapshot:/home/domicor/.mozilla/firefox/7tlpeovq.default/zotero/storage/IGJVVIG2/PhysRevB.37.html:text/html;Full Text PDF:/home/domicor/.mozilla/firefox/7tlpeovq.default/zotero/storage/P4TJDJZB/Tersoff - 1988 - New empirical approach for the structure and energ.pdf:application/pdf}
}

@article{nicholsonYoungs1996,
title = "Young's modulus of diamond-coated fibres and wires ",
journal = {Diamond and Related Materials },
volume = "5",
number = "6-8",
pages = "658 - 663",
year = "1996",
note = "Proceedings of the 6th European Conference on Diamond, Diamond-like and Related Materials Part 2 ",
issn = "0925-9635",
doi = "http://dx.doi.org/10.1016/0925-9635(95)00361-4",
url = "http://www.sciencedirect.com/science/article/pii/0925963595003614",
author = "E.D. Nicholson and T.W. Baker and S.A. Redman and E. Kalaugher and K.N. Rosser and N.M. Everitt and M.N.R. Ashfold and P.G. Partridge",
keywords = "Young's modulus",
keywords = "Diamond",
keywords = "Fibres and wires",
keywords = "Metal matrix composites ",
abstract = "Diamond-coated fibres and wires were produced by hot filament chemical vapour deposition (HFCVD) of diamond on a variety of core materials including tungsten(W) and silicon carbide (SiC). Fibres with a diamond volume fraction exceeding 95% have been produced. Three different methods of measuring the fibre Young's modulus(a resonance method, a bend test and a tensile test) are presented, together with recent results. Possible applications for such fibres include reinforcements in metal matrix composites (MMCs). "
}
